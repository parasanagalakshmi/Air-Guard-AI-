        "#GenerativeAI model that gets user input and provide response and improve the prerformance with feedback\n",
        "# Step 1: Install Required Libraries\n",
        "#!pip install google-generativeai numpy pandas\n",
        "\n",
        "# Step 2: Set Your API Key\n",
        "import os\n",
        "os.environ[\"GOOGLE_API_KEY\"] = 'AIzaSyBfQmb5PfJZywcD1qlP3jcG3BCZbKe5Kgg'\n",
        "\n",
        "# Step 3: Initialize the Gemini API\n",
        "import google.generativeai as genai\n",
        "genai.configure(api_key=os.environ[\"GOOGLE_API_KEY\"])\n",
        "\n",
        "# Step 4: Define the SLM Functionality\n",
        "def slm_model(prompt, feedback=None):\n",
        "    # Construct the prompt for the Gemini API\n",
        "    full_prompt = f\"You are a language model. Respond to the following prompt:\\n{prompt}\\n\"\n",
        "\n",
        "    # If feedback is provided, include it in the prompt\n",
        "    if feedback:\n",
        "        full_prompt += f\"Feedback: {feedback}\\n\"\n",
        "\n",
        "    # Generate a response using the Gemini API\n",
        "    # Create a GenerativeModel instance with a specific model\n",
        "    # Replace 'models/text-bison-001' with the desired model name\n",
        "    # Updated model name to 'models/gemini-pro'\n",
        "    model = genai.GenerativeModel(model_name=\"models/gemini-pro\")\n",
        "\n",
        "    # Call generate_content on the model instance\n",
        "    response = model.generate_content(full_prompt)\n",
        "\n",
        "    return response.text # Extract the text from the response object\n",
        "\n",
        "\n",
        "# Step 5: User Input for Prompts and Feedback\n",
        "prompt = input(\"Please enter your prompt: \")\n",
        "feedback = input(\"If you have feedback for the previous response, please enter it (or press Enter to skip): \")\n",
        "\n",
        "# Step 6: Generate Responses and Incorporate Feedback\n",
        "response = slm_model(prompt, feedback)\n",
        "\n",
        "# Display the response\n",
        "print(\"Generated Response:\")\n",
        "print(response)"
      ]
